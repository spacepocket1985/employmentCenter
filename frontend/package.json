    "@mui/icons-material": "^5.15.13",
    "@mui/material": "^5.15.13",
    "@reduxjs/toolkit": "^2.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "react-redux": "^9.1.0",
    "react-toastify": "^10.0.5",
    "redux": "^5.0.1",
    "uuid": "^9.0.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
